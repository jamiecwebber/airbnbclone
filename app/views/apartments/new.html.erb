<h1>Make A New Apartment</h1>
<p>Find me in app/views/apartments/new.html.erb</p>

<%= simple_form_for @apartment do |f| %>
  <div class= "form-inputs">
    <%= f.input :price_per_day%>
    <%= f.input :category%>
    <%= f.input :name%>
    <%= f.input :description%>
    <%= f.input :location%>
  </div>

  <div class="form-actions">
    <%= f.button :submit%>
  <% end %>
  </div>

  <%= link_to "Return to All Apartments", apartments_path %>
